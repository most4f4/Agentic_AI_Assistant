# API Keys - Replace with your actual keys

# OpenAI API Key (required for GPT-4 and embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# SerpAPI Key (required for web search and stock prices)
SERPAPI_API_KEY=your_serpapi_key_here

# OpenWeather API Key (required for weather tool)
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Optional: LangChain tracing (set to "true" to enable)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your_langchain_api_key_here

# Optional: Firebase Firestore (for cloud storage)
# Path to your Firebase service account JSON key file
GOOGLE_APPLICATION_CREDENTIALS=./firebase-key.json